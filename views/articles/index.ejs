<!DOCTYPE html>
<html>
<head>
    <title>Articles List</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <div class="container">
        <h1>Articles List</h1>
        <ul class="articles-list">
            <% articles.forEach(function(article) { %>
                <li class="article-item">
                    <a href="/articles/<%= article.id %>"><%= article.title %></a>
                </li>
            <% }); %>
        </ul>
        <div class="actions">
            <a href="/">Back to home</a>
        </div>
    </div>
</body>
</html>
